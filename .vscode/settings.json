{
  "eslint.autoFixOnSave": true,
  "files.autoSave": "afterDelay",
  "editor.formatOnSave": true,
  "git.enableSmartCommit": true,
  "files.exclude": {
    "node_modules/": true,
    "yarn.lock": true,
    "package-lock.json": true
  },
  "git.autofetch": true,
  "prettier.eslintIntegration": true,
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "tslint.autoFixOnSave": true,
  "cSpell.words": [
    "bwlines",
    "antialiased",
    "cellpadding",
    "cellspacing",
    "copyfiles",
    "ecmascript",
    "fastify",
    "flushall",
    "gitignore",
    "graphiql",
    "lpush",
    "lrange",
    "lspace",
    "mariadb",
    "nbsp",
    "ormconfig",
    "preheader",
    "proviced",
    "resave",
    "rspace",
    "typeorm",
    "varchar"
  ]
}
